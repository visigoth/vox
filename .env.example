## OpenAI
OPENAI_API_KEY=
OPENAI_REALTIME_MODEL=gpt-realtime
OPENAI_REALTIME_VOICE=
OPENAI_INPUT_AUDIO_FORMAT=g711_ulaw
OPENAI_OUTPUT_AUDIO_FORMAT=g711_ulaw
OPENAI_TRANSCRIPTION_MODEL=gpt-4o-transcribe

## Twilio (required for `vox dial`, optional for inbound hangup tool)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=

## Vox
VOX_PUBLIC_BASE_URL= # e.g. https://your-ngrok-subdomain.ngrok.app
VOX_AGENT_URL=       # optional: POST endpoint for query_agent tool
VOX_AGENT_CMD=       # optional: spawn a local agent CLI instead (JSONL over stdin/stdout), e.g. "node examples/echo-agent.js"
VOX_LOG_DIR=./logs
VOX_INITIAL_GREETING=Hello! This is Vox. How can I help you today?
